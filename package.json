{
  "name": "begin-app",
  "version": "0.0.0",
  "description": "Begin basic Apollo GraphQL API",
  "scripts": {
    "start": "concurrently \"parcel watch src/index.html -d public\" \"cross-env NODE_ENV=testing sandbox\"",
    "test": "cross-env NODE_ENV=testing PORT=6666 tape test/*-test.js ./test/**/*-test.js | tap-spec",
    "build": "parcel build src/index.html -d public"
  },
  "dependencies": {
    "@architect/sandbox": "latest",
    "@begin/data": "latest",
    "cross-env": "^7.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1",
    "tiny-json-http": "^7.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "parcel": "^1.12.4",
    "sandbox": "^0.8.6"
  }
}
